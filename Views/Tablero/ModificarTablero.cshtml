@model ViewTableroUpdate

<form asp-controller="Tablero" asp-action="ModificarTablero">
    <div class="form-group">
        <label asp-for="@Model.Id">Id</label>
        <input asp-for="@Model.Id" value="@Model.Id">
    </div>
    <div class="form-group">
        <label asp-for="@Model.IdUsuarioPropietario">Id Usuario Propietario</label>
        <input asp-for="@Model.IdUsuarioPropietario" value="@Model.IdUsuarioPropietario">
    </div>
    <div class="form-group">
        <label asp-for="@Model.Nombre">Nombre</label>
        <input asp-for="@Model.Nombre" value="@Model.Id">
    </div>
    <div class="form-group">
        <label asp-for="@Model.Descripcion">Descripcion</label>
        <input asp-for="@Model.Descripcion" value="@Model.Descripcion">
    </div>

    <button type="submit">Enviar</button>

</form>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}