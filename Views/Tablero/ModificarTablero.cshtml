@model ViewTableroUpdate

<form asp-controller="Tablero" asp-action="ModificarTablero" method="post">
    <div class="form-group">
        <label asp-for="@Model.Id">Id</label>
        <input asp-for="@Model.Id" value="@Model.Id" required>
    </div>
    <div class="form-group">
        <label asp-for="@Model.IdUsuarioPropietario">Id Usuario Propietario</label>
        <input asp-for="@Model.IdUsuarioPropietario" value="@Model.IdUsuarioPropietario" required>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Nombre">Nombre</label>
        <input asp-for="@Model.Nombre" value="@Model.Nombre" required maxlength="30">
    </div>
    <div class="form-group">
        <label asp-for="@Model.Descripcion">Descripcion</label>
        <input asp-for="@Model.Descripcion" value="@Model.Descripcion" maxlength="100">
    </div>

    <button type="submit">Enviar</button>

</form>
